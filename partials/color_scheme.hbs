{{#match @custom.color_scheme "material"}}
<style>
:root[color-mode='light'] {
  --surface: #fff;
  --surface1: #fafafa;
  --surface2: #f5f5f5;
  --surface3: #eeeeee;
  --surface4: #e0e0e0;
  --text4: #757575;
  --text3: #616161;
  --text2: #424242;
  --text1: #212121;
  --text: #000;

  --primary: #0277BD;
  --secondary: #FF8F00;
  --three: #880E4F;
  --four: #303F9F;
  --five: #33691E;

   --title: var(--surface3);
  --subtitle: var(--surface2);

  --gradient: linear-gradient(
    to right top,
    var(--three),
    var(--primary),
    var(--four),
    var(--secondary),
    var(--five)
  );
  --gradient-reverse: linear-gradient(
    to left top,
    var(--five),
    var(--secondary),
    var(--four),
    var(--primary),
    var(--three)
  );
}

:root[color-mode='dark'] {
  --text: #fff;
  --text1: #fafafa;
  --text2: #f5f5f5;
  --text3: #eeeeee;
  --text4: #e0e0e0;
  --surface4: #757575;
  --surface3: #616161;
  --surface2: #424242;
  --surface1: #212121;
  --surface: #000;

  --primary: #64B5F6;
  --secondary: #FF80AB;
  --three: #9575cd;
  --four: #dce775;
  --five: #ffd54f;

  --title: var(--text);
  --subtitle: var(--text4);

  --gradient: linear-gradient(
    to right top,
    var(--three),
    var(--primary),
    var(--four),
    var(--secondary),
    var(--five)
  );
  --gradient-reverse: linear-gradient(
    to left top,
    var(--five),
    var(--secondary),
    var(--four),
    var(--primary),
    var(--three)
  );
}
</style>
{{/match}}

